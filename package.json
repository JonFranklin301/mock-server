{
  "name": "mock-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "pkg": "npm run pkg-win-x64 & npm run pkg-win-x86",
    "pkg-win-x64": "pkg ./bin/www --output ./dist/mock-server-x64.exe --target node12-win-x64",
    "pkg-win-x86": "pkg ./bin/www --output ./dist/mock-server-x86.exe --target node12-win-x86"
  },
  "dependencies": {
    "commander": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "node-json-color-stringify": "^1.1.0",
    "prettyjson": "^1.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4",
    "pkg": "^4.4.4"
  }
}
